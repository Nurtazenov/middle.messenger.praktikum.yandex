{
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": "node_modules/",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "airbnb-typescript/base"
  ],
  "rules": {
    "no-unused-vars": 2,
    "max-len": [
      1,
      100
    ],
    "max-params": [
      2,
      3
    ]
  }
}
       






