{
"plugins": ["@typescript-eslint"],
  "ignorePatterns": "node_modules/",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb"
  ],
  "rules": {
    "no-unused-vars": 2,
    "max-len": [
      1,
      100
    ],
    "max-params": [
      2,
      3
    ]
  }
}
