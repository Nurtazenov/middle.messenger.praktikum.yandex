{
  "extends": [
    "airbnb-typescript/base", 
    "plugin:@typescript-eslint/recommended"
],
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
  },
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
  },
  "rules": {
      "no-console": "warn", 
      "indent": ["error", 4]
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
}
