{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "number-no-trailing-zeros": true,
    "string-no-newline": true,
    "color-no-invalid-hex": true,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extend", "include", "mixin"]
      }
    ]
  }
}



