{
  "extends": [
  "stylelint-plugin-bem",
"stylelint-config-recommended-scss"],
  "plugins": [
    "stylelint-plugin-bem"
  ],
  "rules": {
    "no-descending-specificity": null,
    "scss/dollar-variable-pattern": null,
    "scss/at-if-no-null": null,
    "plugin/selector-bem-pattern": {
      "preset": "bem",
      "componentSelectors": "bemSelector"
    }
  },
  "ignoreFiles": "['build]"
}


